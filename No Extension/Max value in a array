//Program to find largest element in array
#include<iostream>
using namespace std;
int temp[5] = { 2,5,9,7,3 };
int main()
{
	cout << "Printing array: " << endl;
	for (int i = 0; i < 5; i++)
	{
		cout << temp[i] << " ";
	}
	cout << endl;
	int max = temp[0];
	for (int j = 1; j < 5; j++)
	{
		if (temp[j]>max)
		{
			max = temp[j];
		}
		else
		{ }
	}
	cout << "Max value in the array is: " << max << endl;
	system("pause");
	return 0;
}
