#include <iostream> 
using namespace std; 
 
unsigned long long fact(unsigned long long  n) 
{ 
    if (n == 0) 
        return 1; 
    return n * fact(n - 1); 
} 
  

int main() 
{ 
    unsigned long long n;
    
    cin>>n;
    
    cout << "Factorial of "<< n<< " is " << fact(n) << endl; 
    return 0; 
} 
