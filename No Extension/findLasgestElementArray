function maxValue(array){
  //console.log(array.length);
  max = array[0];
  //console.log(max);
  for (i=0; i<array.length; i++){
    //console.log(array[i]);
    if (array[i] > max) {
      max = array[i];
    }
    
  }
  return max;
}

//Testing
const array = [1,2,4,3,12,8,4,9];
maxx= maxValue(array);
console.log(maxx);
