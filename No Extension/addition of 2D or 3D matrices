#include<iostream>
using namespace std;
int main()
{
int r;
cout<<"enter number of row 2/3\n";
cin>>r;
int a[r][r],b[r][r],c[r][r];

cout<<"enter 1st matrix elements\n";
for(int i=0;i<r;i++)
{
for(int j=0;j<r;j++)
{
cin>>a[i][j];
cout<<"\t";
}
cout<<endl;}
cout<<"enter 2nd matrix elements\n";
for(int i=0;i<r;i++)
{
for(int j=0;j<r;j++)
{
cin>>b[i][j];
cout<<"\t";
}
cout<<endl;}

for(int i=0;i<r;i++)
{
for(int j=0;j<r;j++)
{
c[i][j]=a[i][j]+b[i][j];
}
}


for(int i=0;i<r;i++)
{
for(int j=0;j<r;j++)
{
cout<<c[i][j];

    cout<<"\t";
}
cout<<endl;}

return 0;

}
