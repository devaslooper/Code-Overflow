//reverse of an array
#include<stdio.h>
int main()
{
	int a[5],i,j,t;
	printf("enter 6 values");
	for(i=0;i<=4;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=0,j=4;i<j;i++,j--)
	{
		t = a[i];
		a[i] = a[j];
		a[j] = t;
	}
	printf("reverse array is --->");
	for(i=0;i<=4;i++)
	{
		printf("%d\n",a[i]);
	}
	return 0;
}
