#include <stdio.h>

int main()
{
    int a[3][3],b[3][3];
    
    printf("Enter Elements in Array 1");
    for(int i=0;i<3;i++) {
        for(int j=0;j<3;j++) {
            printf("Enter a[%d][%d] = ",i,j);
            scanf("%d",&a[i][j]);
        }
    }
    
    printf("Enter Elements in Array 2");
    for(int i=0;i<3;i++) {
        for(int j=0;j<3;j++) {
            printf("Enter b[%d][%d] = ",i,j);
            scanf("%d",&b[i][j]);
        }
    }

    printf("Sum of 3x3 array is \n");
    for(int i=0;i<3;i++) {
        for(int j=0;j<3;j++) {
            printf("%d",a[i][j]+b[i][j]);
        }
        printf("\n");
    }
}
