#include<iostream>
#include<conio.h>
using namespace std;

int main()
{
    int arr[50], n, sum = 0;
    cout<<"enter the number of elements:"<<endl;
    cin>>n;
    cout<<"enter the elements of the array"<<endl;
    for(int i = 0; i<n; i++)
    {
        cin>>arr[i];
        sum = sum + arr[i];
    }
    cout<<"sum of the numbers in the array : "<<sum;

    return 0;

}
