#include<stdio.h>
int main()
{
  int a[5]={1,2,3,4,5},i,n=5;
  printf("original array=");
  for(i=0;i<5;i++)
   {
     scanf("%d",&a[i]);
   }

  for(i=0;i<5;i++)
        {
          printf("%d",a[i]);
        }
    printf("\n");
    printf("reverse array=");
  for(i=n-1;i>=0;i--)
        {
        printf("%d",a[i]);
        }
  return 0;
}
