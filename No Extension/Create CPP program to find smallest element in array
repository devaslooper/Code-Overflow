#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    
    cout<<"Enter the no. of elemnts in your array\n";
    
    cin>>n;
    
    int a[n];
    
    cout<<"Enter the elemnts\n";
    
    for(int i=0;i<n;i++)
     cin>>a[i];
     
    cout<<"The minimum elemnt in the array is\n";
    
    cout<<(*min_element(a,a+n));
    
    return 0;
       
    
}  

