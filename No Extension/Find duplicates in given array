#include <bits/stdc++.h> 
using namespace std; 

int main() 
{ 
    int n;
    cout<<"Enter size of array"<<endl;
    cin>>n;
    
    cout<<"Enter elements of array"<<endl;
    int arr[n];
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
    
    for(int i=0;i<n;i++){
        if (arr[abs(arr[i])] >= 0){
            arr[abs(arr[i])] = -arr[abs(arr[i])]; 
        }
        else{
          cout << abs(arr[i]) << " ";
        }
    }
    return 0; 
} 
