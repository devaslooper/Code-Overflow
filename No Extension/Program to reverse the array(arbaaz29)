#include<stdio.h>
static int m=10000;
int main(){
int a[m],b[m];
int i,j,n;
printf("number of integers you want to enter:");
scanf("%d", &n);
for(i=0;i<n;i++){
  scanf("%d", &a[i]);
}
i=0;
for(j=n;j>0;j--){
  b[j]=a[i];
  i++;
}
printf("\t\tThe reversed array is as follows:\n");
for(j=1;j<=n;j++){
printf("\t\t\n %d", b[j]);
}
return 0;
}
