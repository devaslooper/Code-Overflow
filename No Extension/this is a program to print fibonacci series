#include <stdio.h>
int main() {
    int i, n, t1 = 0, t2 = 1, next;
    printf("Enter the number of terms: ");
    scanf("%d", &n);
    printf("\n The Fibonacci Series: ");

    for (i = 0; i <= n-1; i++) {
        printf("%d, ", t1);
        next = t1 + t2;
        t1 = t2;
        t2 = next;
    }

    return 0;
}
