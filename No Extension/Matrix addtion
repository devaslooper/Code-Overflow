#include<stdio.h>
void main()
{
  int a[100][100],b[100][100],row,col,sum[100][100],i,j;
  printf("Enter no of rows and cols");
  scanf("%d%d",&row,&col);
  printf("Enter Matrix A");
  for(i=0;i<row;i++)
  {for(j=0;j<col;j++)
      scanf("%d",&a[i][j]);
  }
  printf("Enter Matrix B");
  for(i=0;i<row;i++)
  {for(j=0;j<col;j++)
      scanf("%d",&b[i][j]);
  }
 
 if(row==col)
  { for(i=0;i<row;i++)
    {for(j=0;j<col;j++)
      sum[i][j]=a[i][j]+b[i][j];
    }
  }
  else
  printf("Matrix can not be added");
  printf("Matrix Addition is"
 for(i=0;i<row;i++)
  {for(j=0;j<col;j++)
      printf("%d",sum[i][j]);
  }
  
}
