function factorialNumber(num){

  // If the number is less than 0, reject it.
  if (num < 0) 
    return -1;

  else if (num==1)
    return 1;
  
  else{
    return num * factorialNumber(num-1);
  }
  
  
}

// test = factorialNumber(10);
number = 10;
finalFactorial=factorialNumber(number);
console.log(finalFactorial);
