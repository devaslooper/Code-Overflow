public class Largest {

    public static void main(String[] args) {
        
        
        int[] numFirstArray = { 3,6,8,9,6,2};
        
        int largest = numFirstArray[0];

        for (int num: numFirstArray) {
            if(largest < num)
                largest = num;
        }

        System.out.format("Largest element in array : "+ largest);
    }
}
