#include<stdio.h>
int main()
{
	int n=6;
	int i;
	int array[6];
	for(i=0;i<6;i++){
		scanf("%d",&array[i]);
	}
	for(i=0;i<6;i++){
		printf("The array is\n%d\n",array[i]);	
	}
printf("ENter the element position u want to delete\n");
int pos;
scanf("%d",&pos);	
for(i=pos;i<=5;i++){
	array[i]=array[i+1];
}
for(i=0;i<6;i++)
{
	printf("The array after deletion\n%d\n",array[i]);
}
n=n-1;
for(i=0;i<n;i++)
{
	printf("The true array after deletion\n%d\n",array[i]);
}
}
