#include<stdio.h>
#include<conio.h>
#include<math.h>
#include<bits/stdc++.h>

using namespace std;

int main()
{
    int b,d=0,i=0,s;
    cin>>b;
    while(b!=0)
    {

        s=b%10;
        b/=10;
        if(s!=0&&s!=1)
        {
            break;
        }
        d=d+(s*pow(8,i++));
    }
    if(s!=0&&s!=1)
        cout<<"not a binary";
    else
        cout<<"Decimal Conversion is : "<<d;
 return 0;
}
