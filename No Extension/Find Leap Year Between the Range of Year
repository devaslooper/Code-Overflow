#include<iostream>
using namespace std;
int main(int argc, char const *argv[])
{
    int range1, range2,count=0;
    cout<<"Enter the Year Range which you want to Count number of Leap Years\n";
    cin>>range1>>range2;
    while(range1<=range2){
        if(range1%4==0){
            count++;
            range1=range1+4;
        }
        else{
            range1++;
        }
    }
    cout<<"\nThe Number of Leap Year Between this Range is : "<<count;
    return 0;
}
