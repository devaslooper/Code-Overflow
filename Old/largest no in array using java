import java.io.*;
public class largest 
{
public static void main(String args[])
{
Scanner sc= new Scanner(System.in)
{
int n=sc.nextInt()  //size of array
int arr[]= new int[n]  //array creation
for(int i=0;i<n;i++)
arr[i]= sc.nextInt();
int max= arr[0];
for(int i=0;i<max;i++)
{
if(arr[i]>max)
max=arr[i];
}
System.out.println("the maximum no is"+max);
}
}
