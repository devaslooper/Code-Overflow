function minValue(array){
  //console.log(array.length);
  min = array[0];
  //console.log(min);
  for (i=0; i<array.length; i++){
    //console.log(array[i]);
    if (array[i] < min) {
      min = array[i];
    }
    
  }
  return min;
}

//Testing
const array = [5,2,4,3,12,8,4,9];
minimum= minValue(array);
console.log(minimum);
