#include<stdio.h>
#include<conio.h>
#include<math.h>
void distance_points(int,int,int,int,int,int);
void main()
{
float a,b,c,d,e,f,k,distancepoints;
float j;
printf("enter your 1st point's cordinates i.e;(x1,x2,x3) :\n ");
scanf("%f%f%f",&a,&b,&c);
printf("enter your 2nd point's cordinates i.e;(y1,y2,y3) :\n ");
scanf("%f%f%f",&d,&e,&f);
distancepoints=sqrt(((d-a)*(d-a))+((e-b)*(e-b))+((f-c)*(f-c)));
j=distancepoints;
printf("the distance between two points in space is : %f",j);
getch();
}
void distance_points(inta,intb,intc,intd,inte,intf)
{
float a,b,c,d,e,f,distancepoints;
float j;
distancepoints=sqrt(((d-a)*(d-a))+((e-b)*(e-b))+((f-c)*(f-c)));
j=distancepoints;
return(j);
}
