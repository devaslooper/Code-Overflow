#include <stdio.h>
#include <stdlib.h>
int main()
{
    
    int size;
    scanf("%d",&size);
    int* array = (int*)malloc(size*sizeof(int));
    int i;
    for(i=0;i<size;i++)
     scanf("%d",&array[i]);
    int max =-1;
    for(i=0;i<size;i++)
    {
        if(array[i]>max)
        max=array[i];
    }
    printf("%d",max);
    return 0;
}
