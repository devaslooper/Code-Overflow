#include <stdio.h>
#define MAX_SIZE 100

int main()
{
  int arr[MAX_SIZE];
  int i, n, sum=0;
  
  /*input size of the array*/
  printf("Enter size of the array: ");
  scanf("%d", &n);
  
  /*input element in array*/
  printf("Enter %d element in the aray: ",n);
  for(i=0; i<n; i++)
  {
    scanf("%d", &arr[i]);
  }
  
  /*Add each array element to sum*/
  for(i=0; i<n; i++)
  {
    sum = sum + arr[i];
  }
  
  printf("Sum of all elements of array = %d" , sum);
  return 0:
}
