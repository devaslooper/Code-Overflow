# Program to calculate count of vowels & consonants in string

name = str(input("Enter the string: "))
vowels = 0
consonants = 0
for i in range(len(name)):
    if name[i] == 'a' or name[i] == 'e' or name[i] == 'i' or name[i] == 'o' or name[i] == 'u' or name[i] == 'A' or name[i] == 'E' or name[i] == 'I' or name[i] == 'O' or name[i] == 'U':
        vowels = vowels + 1
    else:
        consonants = consonants + 1
print("Count of Vowels is : ",vowels)
print("Count of Consonants is : ",consonants)
