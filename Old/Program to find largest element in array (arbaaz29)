#include<stdio.h>
static int M=100000;
int main(){
int a[M];
int i,n;
int temp,larg;
printf("enter the number of elements you want to enter:");
scanf("%d", &n);
for(i=0;i<n;i++){
  scanf("%d", &a[i]);
}
larg=a[0];
for(i=0;i<n;i++){
    if(a[i]>larg){
        temp=larg;
        larg=a[i];
        a[i]=temp;
    }
}
printf("largest value=%d", larg);
return 0;
}
