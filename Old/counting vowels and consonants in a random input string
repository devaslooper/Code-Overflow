s = input()   # taking input string
l = len(s)    # calculate length of string
# creating list of vowels
num = ['a','A','e','E','i','I','o','O','u','U']
count_vowels = 0
count_consonants = 0
for i in s:
    if i in num:
        count_vowels += 1
    elif 64<ord(i)<91 or 96<ord(i)<123:
        count_consonants += 1
print(count_vowels)
print(count_consonants)
