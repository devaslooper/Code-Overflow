#include<stdio.h>
void main(){
    int a[100],n,key,i;
    printf("enter number of elements");
    scanf("%d",&n);
    printf("enter the elements");
    for (i = 0; i < n ; i++)
    {
     scanf("%d",&a[i]);   
    }
    printf("enter key value");
    scanf("%d",&key);
    for (i = 0; i < n; i++)
    {

        if (a[i]==key)
        {
            printf("element found at %d position",i+1);
        }
    }

}
