#include<bits/stdc++.h>
using namespace std;
#define ll long long
ll solution(ll n)
{
    cout<<"Enter array elements\n";
    ll maximum, temp;
    cin>> maximum;  n--;
    while (n--)
    {
        cin>>temp;
        maximum = max(maximum, temp);
    }
    return maximum;
}

int main()
{
    ios_base::sync_with_stdio(false); cin.tie(NULL); cout.tie(NULL);
    cout<<"Enter the size of array\n";
    ll n;   cin>>n;
    {
        cout<<solution(n)<<"\n";
    }
    return 0;
}
