import java.util.*; 

class Solution { 

static int fib(int n) { 
double result = (1 + Math.sqrt(5)) / 2; 
return (int) Math.round(Math.pow(result, n)  
                        / Math.sqrt(5)); 
} 

// Driver Code 
public static void main(String[] args) { 
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
    System.out.println(fib(n)); 
    } 
} 
