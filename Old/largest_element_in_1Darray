lst = []
# number of elements as input
n = int(input("Enter number of elements : "))
# iterating till the range
for i in range(0, n):
    ele = int(input())

    lst.append(ele)  # adding the element
def largest(lst,n):
	# Initialize maximum element
	max = lst[0]
	for i in range(1, n):
		if lst[i] > max:
			max = lst[i]
	return max
Ans = largest(lst,n)
print ("Largest in given array is",Ans)
