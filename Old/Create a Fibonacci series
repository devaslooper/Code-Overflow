#include<iostream>
using namespace std;
int main()
{
      long long int  first=0, second=1, i, n, sum=0;
      cout<<"Enter the number of terms: "; 
      cin>>n;
      cout<<"Fibonacci Series: ";
  
      for(i=0 ; i<n ; i++)
     {  
               if(i <= 1)
      {
           sum=i;
                }
                  
                else
                {
                      sum=first + second;
             first=second;
                      second=sum;
                }
     cout<<sum<<" ";
      }  
    return 0;
}
