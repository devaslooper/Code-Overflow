import java.util.*;
public class Main
{
	public static void main(String[] args) {
	 String str = "Helloworld";
	 String vowels = "AEIOUaeiou";
	 int len = str.length();
	 int vow = 0;
	 int con = 0;
	 for(int i=0;i<len;i++){
	     if(vowels.indexOf(str.charAt(i))!=-1) vow++;
	     else con++;
	 }
	 System.out.println("Vowels = "+vow);
	 System.out.println("consonants = "+con);
}
    
}
