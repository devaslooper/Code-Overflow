#include<stdio.h>
#include<conio.h>
int calculator(int,int);
void main()
{
int n;
float culator,num1,num2,multiply,sum,sub,result;
float division;
printf("enter your FIRST NUMBER : ");
scanf("%f",&num1);
printf("enter your SECOND NUMBER : ");
scanf("%f",&num2);
printf("enter a nuber between 1 to 4 to perform maths operation :\n 1:-SUM \n 2:-SUB \n 3:-MULTIPLY \n 4:-DIVISION\n YOUR RESPONSE : ");
scanf("%d",&n);
switch(n)
{
case 1:
  sum=num1+num2;
  printf(" the sum is =%f",sum);
  break;
case 2:
  sub=num1-num2;
  printf("the sub is =%f",sub);
  break;
case 3:
  multiply=num1*num2;
  printf("the multiply is =%f",multiply);
  break;
case 4:
  division=num1/num2;
  printf("the division is =%f",division);
  break;
default:
  printf("enter a valid no. between 1 to 4");
  break;
printf("mathematical result of the inputed number %f",result);
}

getch();
}
int calculator (num1 ,num2)
{
float result,n;


return(result);
}
