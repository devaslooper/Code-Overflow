
  
#include <bits/stdc++.h> 
  
using namespace std; 
 
 
int calNum(int year) 
{ 
    return (year / 4) - (year / 100) + 
                        (year / 400); 
} 
  

void leapNum(int l, int r) 
{ 
    l--; 
    int num1 = calNum(r); 
    int num2 = calNum(l); 
    cout << num1 - num2 << endl; 
} 
  

int main() 
{ 
    int l1 = 1, r1 = 400; 
    leapNum(l1, r1); 
  
    int l2 = 400, r2 = 2000; 
    leapNum(l2, r2); 
  
    return 0; 
} 
