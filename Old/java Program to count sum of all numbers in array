import java.utils.*;
public class sum
{
public static void main(String args[])
{
Scanner sc= new Scanner(System.in);
int n= sc.nextInt(); //size
int arr[]= new int[n];
count=0;
for(int i=0;i<n;i++)
{
arr[i]=sc.nextInt();
count=count+arr[i];
}
System.out.println("Sum of all the numbers of array is",+count);
}
}
