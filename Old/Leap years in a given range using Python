
Years = [2005, 2006, 2007, 2008, 2009, 2010, 
         2011, 2012, 2013, 2014, 2015, 2016] 
  
# Find whether it is leap year or not 
def checkYear(year): 
    return (((year % 4 == 0) and
             (year % 100 != 0)) or
             (year % 400 == 0))  
   
# leap_year Initialization 
leap_year = 0 
  
for elem in Years: 
    if checkYear(elem): 
        leap_year = leap_year + 1
  
print("No of leap years are:", leap_year)
