lst = []
# number of elements as input
n = int(input("Enter number of elements : "))
# iterating till the range
for i in range(0, n):
    ele = int(input())

    lst.append(ele)  # adding the element
def smallest(lst,n):
	# Initialize smallest element
	min = lst[0]
	for i in range(1, n):
		if lst[i] < min:
			min = lst[i]
	return min
Ans = smallest(lst,n)
print ("Smallest in given array is",Ans)
